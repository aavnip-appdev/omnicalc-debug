<form action="/calendar_invite/results">
  
  <div>
    <label for="event_name">Name</label>
    <input type="text" id="event_name" name="name">
  </div>
  <div>
    <label for="event_details">Details</label>
    <input type="text" id="event_details" name="details">
  </div>
  <div>
    <label for="event_location">Location</label>
    <input type="text" id="event_location" name="location">
  </div>
  <div>
    <label for="event_begins">Start time</label>
    <input type="datetime-local" id="event_begins" name="start_date">
  </div>
  <div>
    <label for="event_ends">End time</label>
    <input type="datetime-local" id="event_ends" name="end_date">
  </div>
  <button>Generate</button>
</form>
